server {
       listen 80;
       server_name 3.127.223.122;
       root /home/ubuntu;

       location /api/v1 {
                proxy_set_header Host $http_host;
                proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;

                proxy_pass http://127.0.0.1:8000;
       }

       location / {
              try_files /index.html;
       }
}
